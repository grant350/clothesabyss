/*================ Modals ================*/
.modal {
  @include transform(translateY(-20px));
  background-color: $color-bg;
  color: black;
  display: none;
  opacity: 0;
  overflow: hidden;
  position: relative;
  margin:0px; padding:0px;
}

.modal--is-active {
  @include transform(translateY(0));
  @include maxw-flexbox();
  @include flex-wrap(wrap);
  min-width:40px;
  min-height:40px;
  opacity: 1;
  overflow: hidden;
}

.modal__inner {
  @include prefix(transform-style, preserve-3d, moz webkit spec);
  height: 100%;
}

.modal__centered {
  @include transform(translateY(-50%));
  position: relative;
  top: 50%;

  .no-csstransforms & {
    top: 20%;
  }
}

.modal__close {
  border: 0;
  //padding: $gutter-site;
  position: fixed;
  top: 0;
  right: 0;
  z-index: 2;
  position: absolute;
  top: 0px;
  right: 0px;
  height: 30px;
  width: 30px;
  color: black;
  margin: 40px;
  
  .icon {
    font-size: em(20);
  }
}

