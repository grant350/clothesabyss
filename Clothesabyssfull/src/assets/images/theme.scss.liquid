}

body,input,textarea,button,select {
  -webkit-font-smoothing: antialiased;
  -webkit-text-size-adjust: 100%;
}

a {
  background-color: transparent;
}

b,strong {
  font-weight: 300;
}

small {
  font-size: 80%;
